<?xml version="1.0" encoding="UTF-8"?>
<!-- schema for odwn and orbn including wordNet grid references -->
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="LexicalResource">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="GlobalInformation"/>
        <xs:element ref="Lexicon"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GlobalInformation">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.GlobalInformation"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.GlobalInformation">
    <xs:attribute name="label" use="required"/>
  </xs:attributeGroup>
  <xs:element name="Lexicon">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="LexicalEntry"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Synset"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.Lexicon"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Lexicon">
    <xs:attribute name="label" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="language" use="required" type="xs:NMTOKEN"/>
    <xs:attribute name="languageCoding" use="required"/>
    <xs:attribute name="owner" use="required" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <xs:element name="LexicalEntry">
    <xs:complexType>
      <xs:sequence>
        <xs:choice>
          <xs:sequence>
            <xs:element ref="Lemma"/>
            <xs:element ref="WordForms"/>
          </xs:sequence>
          <xs:element ref="MultiwordExpression"/>
        </xs:choice>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="RelatedForms"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Morphology"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="MorphoSyntax"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SyntacticBehaviour"/>
        <xs:element ref="Sense"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.LexicalEntry"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.LexicalEntry">
    <xs:attribute name="id" use="required"/>
    <xs:attribute name="partOfSpeech">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="adverb"/>
          <xs:enumeration value="adjective"/>
          <xs:enumeration value="noun"/>
          <xs:enumeration value="verb"/>
          <xs:enumeration value="other"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="formType">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="contraction"/>
          <xs:enumeration value="acronym"/>
          <xs:enumeration value="abbreviation"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="Lemma">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.Lemma"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Lemma">
    <xs:attribute name="writtenForm" use="required"/>
    <xs:attribute name="mode">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="infinitive"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!---->
  <xs:element name="MultiwordExpression">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.MultiwordExpression"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.MultiwordExpression">
    <xs:attribute name="writtenForm" use="required"/>
    <xs:attribute name="expressionType">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="idiom"/>
          <xs:enumeration value="proverb"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!---->
  <xs:element name="WordForms">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="WordForm"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="WordForm">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.WordForm"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.WordForm">
    <xs:attribute name="article"/>
    <xs:attribute name="grammaticalNumber">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="plural"/>
          <xs:enumeration value="singular"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="comparison">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="comparative"/>
          <xs:enumeration value="superlative"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="tense">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="pastTense"/>
          <xs:enumeration value="pastParticiple"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="writtenForm" use="required"/>
  </xs:attributeGroup>
  <!---->
  <xs:element name="RelatedForms">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="RelatedForm"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="RelatedForm">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.RelatedForm"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.RelatedForm">
    <xs:attribute name="writtenForm" use="required"/>
    <xs:attribute name="variantType" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="formVariant"/>
          <xs:enumeration value="spellingVariant"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!---->
  <xs:element name="Morphology">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.Morphology"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Morphology">
    <xs:attribute name="morphoType">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="compderiv"/>
          <xs:enumeration value="derivation"/>
          <xs:enumeration value="compound"/>
          <xs:enumeration value="zero-derivation"/>
          <xs:enumeration value="x-compound"/>
          <xs:enumeration value="wordgroup"/>
          <xs:enumeration value="phrasal"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="comparisonType">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="regular"/>
          <xs:enumeration value="irregular"/>
          <xs:enumeration value="mixed"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="declinable">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="yes"/>
          <xs:enumeration value="no"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="separability">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="separable"/>
          <xs:enumeration value="inseparable"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!---->
  <xs:element name="MorphoSyntax">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="auxiliaries"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.MorphoSyntax"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.MorphoSyntax">
    <xs:attribute name="pronominalAndGrammaticalGender">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="f"/>
          <xs:enumeration value="m"/>
          <xs:enumeration value="n"/>
          <xs:enumeration value="fn"/>
          <xs:enumeration value="m_f"/>
          <xs:enumeration value="mfn"/>
          <xs:enumeration value="mn"/>
          <xs:enumeration value="mf"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="adverbialUsage">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="yes"/>
          <xs:enumeration value="no"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="position">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="attributive"/>
          <xs:enumeration value="predicative"/>
          <xs:enumeration value="attrpred"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="reflexivity">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="optionalReflexive"/>
          <xs:enumeration value="reflexive"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!---->
  <xs:element name="auxiliaries">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.auxiliaries"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.auxiliaries">
    <xs:attribute name="auxiliary">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="hebben"/>
          <xs:enumeration value="zijn"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!---->
  <xs:element name="SyntacticBehaviour">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Complementation"/>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="SyntacticSubcategorisationFrame"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.SyntacticBehaviour"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.SyntacticBehaviour">
    <xs:attribute name="valency">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="mono"/>
          <xs:enumeration value="di"/>
          <xs:enumeration value="tri"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="transitivity">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="transitive"/>
          <xs:enumeration value="intransitive"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!---->
  <xs:element name="Complementation">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.Complementation"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Complementation">
    <xs:attribute name="complement">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="datclause"/>
          <xs:enumeration value="ofclause"/>
          <xs:enumeration value="dancomp"/>
          <xs:enumeration value="whclause"/>
          <xs:enumeration value="oblobj"/>
          <xs:enumeration value="quant"/>
          <xs:enumeration value="oblprep"/>
          <xs:enumeration value="factive"/>
          <xs:enumeration value="fixprep"/>
          <xs:enumeration value="omtoinf"/>
          <xs:enumeration value="prep"/>
          <xs:enumeration value="psmodnoun"/>
          <xs:enumeration value="toinf"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="preposition">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="wegens"/>
          <xs:enumeration value="te"/>
          <xs:enumeration value="zonder"/>
          <xs:enumeration value="achter"/>
          <xs:enumeration value="ter...van"/>
          <xs:enumeration value="met...van"/>
          <xs:enumeration value="in...van"/>
          <xs:enumeration value="met...tot"/>
          <xs:enumeration value="op...bij"/>
          <xs:enumeration value="per"/>
          <xs:enumeration value="aangaande"/>
          <xs:enumeration value="langs"/>
          <xs:enumeration value="inzake"/>
          <xs:enumeration value="boven"/>
          <xs:enumeration value="door"/>
          <xs:enumeration value="binnen"/>
          <xs:enumeration value="via"/>
          <xs:enumeration value="ten...van"/>
          <xs:enumeration value="uit"/>
          <xs:enumeration value="omtrent"/>
          <xs:enumeration value="om"/>
          <xs:enumeration value="aan"/>
          <xs:enumeration value="als"/>
          <xs:enumeration value="bij"/>
          <xs:enumeration value="in"/>
          <xs:enumeration value="jegens"/>
          <xs:enumeration value="met"/>
          <xs:enumeration value="naar"/>
          <xs:enumeration value="onder"/>
          <xs:enumeration value="onder...van"/>
          <xs:enumeration value="op"/>
          <xs:enumeration value="over"/>
          <xs:enumeration value="rond"/>
          <xs:enumeration value="tegen"/>
          <xs:enumeration value="tegenover"/>
          <xs:enumeration value="tot"/>
          <xs:enumeration value="tussen"/>
          <xs:enumeration value="van"/>
          <xs:enumeration value="voor"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:element name="SyntacticSubcategorisationFrame">
    <xs:complexType>
      <xs:sequence>
        <xs:element maxOccurs="unbounded" ref="syntacticArgument"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="syntacticArgument">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.syntacticArgument"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.syntacticArgument">
    <xs:attribute name="constituent">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="nil"/>
          <xs:enumeration value="np"/>
          <xs:enumeration value="pp"/>
          <xs:enumeration value="subclause"/>
          <xs:enumeration value="ap"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="function">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="nil"/>
          <xs:enumeration value="specifyingComplement"/>
          <xs:enumeration value="objectComplement"/>
          <xs:enumeration value="directObject"/>
          <xs:enumeration value="indirectObject"/>
          <xs:enumeration value="prepositionalObject"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="preposition"/>
    <xs:attribute name="complementizer">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="dat"/>
          <xs:enumeration value="omte"/>
          <xs:enumeration value="te"/>
          <xs:enumeration value="WH"/>
          <xs:enumeration value="hoe"/>
          <xs:enumeration value="of"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!---->
  <xs:element name="Sense">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Sentiment"/>
        <xs:element ref="Pragmatics"/>
        <xs:choice>
          <xs:element ref="Semantics-verb"/>
          <xs:element ref="Semantics-noun"/>
          <xs:element ref="Semantics-adjective"/>
        </xs:choice>
        <xs:element ref="SenseExamples"/>
        <xs:element ref="SenseRelations"/>
      </xs:choice>
      <xs:attributeGroup ref="attlist.Sense"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Sense">
    <xs:attribute name="id" use="required"/>
    <xs:attribute name="senseId"/>
    <xs:attribute name="provenance"/>
    <xs:attribute name="synset" type="xs:NMTOKEN"/>
    <xs:attribute name="definition" use="required"/>
  </xs:attributeGroup>
  <!---->
  <xs:element name="Sentiment">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.Sentiment"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Sentiment">
    <xs:attribute name="polarity" use="required">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="negative"/>
          <xs:enumeration value="positive"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="externalReference" use="required" type="xs:NMTOKEN"/>
  </xs:attributeGroup>
  <!---->
  <xs:element name="Pragmatics">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="Domains"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.Pragmatics"/>
    </xs:complexType>
  </xs:element>
  <xs:element name="Domains">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.Domains"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Domains">
    <xs:attribute name="domain">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="accoustics"/>
          <xs:enumeration value="administration"/>
          <xs:enumeration value="aeronautics"/>
          <xs:enumeration value="agriculture"/>
          <xs:enumeration value="alimentation"/>
          <xs:enumeration value="anatomy"/>
          <xs:enumeration value="anthropology"/>
          <xs:enumeration value="archeology"/>
          <xs:enumeration value="architecture"/>
          <xs:enumeration value="art"/>
          <xs:enumeration value="artisanship"/>
          <xs:enumeration value="astrology"/>
          <xs:enumeration value="astronomy"/>
          <xs:enumeration value="astronautics"/>
          <xs:enumeration value="astronomie"/>
          <xs:enumeration value="biochemistry"/>
          <xs:enumeration value="biology"/>
          <xs:enumeration value="botany"/>
          <xs:enumeration value="building_industry"/>
          <xs:enumeration value="chemistry"/>
          <xs:enumeration value="cinema"/>
          <xs:enumeration value="commerce"/>
          <xs:enumeration value="computer_science"/>
          <xs:enumeration value="cycling"/>
          <xs:enumeration value="dance"/>
          <xs:enumeration value="diplomacy"/>
          <xs:enumeration value="doctrines"/>
          <xs:enumeration value="ecology"/>
          <xs:enumeration value="economy"/>
          <xs:enumeration value="electronics_electricity"/>
          <xs:enumeration value="empty"/>
          <xs:enumeration value="engineering"/>
          <xs:enumeration value="fashion"/>
          <xs:enumeration value="fishing"/>
          <xs:enumeration value="folklore"/>
          <xs:enumeration value="gastronomy"/>
          <xs:enumeration value="geography"/>
          <xs:enumeration value="geology"/>
          <xs:enumeration value="golf"/>
          <xs:enumeration value="herladry"/>
          <xs:enumeration value="history"/>
          <xs:enumeration value="housekeeping"/>
          <xs:enumeration value="hunting"/>
          <xs:enumeration value="hydraulics"/>
          <xs:enumeration value="industry"/>
          <xs:enumeration value="insurance"/>
          <xs:enumeration value="law"/>
          <xs:enumeration value="linguistics"/>
          <xs:enumeration value="literature"/>
          <xs:enumeration value="mathematics"/>
          <xs:enumeration value="media"/>
          <xs:enumeration value="medicine"/>
          <xs:enumeration value="merchant_navy"/>
          <xs:enumeration value="meteorology"/>
          <xs:enumeration value="metrology"/>
          <xs:enumeration value="military"/>
          <xs:enumeration value="money"/>
          <xs:enumeration value="music"/>
          <xs:enumeration value="mythology"/>
          <xs:enumeration value="pedagogy"/>
          <xs:enumeration value="pharmacy"/>
          <xs:enumeration value="philosophy"/>
          <xs:enumeration value="photography"/>
          <xs:enumeration value="physics"/>
          <xs:enumeration value="play"/>
          <xs:enumeration value="politics"/>
          <xs:enumeration value="psychology"/>
          <xs:enumeration value="publishing"/>
          <xs:enumeration value="railway"/>
          <xs:enumeration value="religion"/>
          <xs:enumeration value="school"/>
          <xs:enumeration value="science"/>
          <xs:enumeration value="sculpture"/>
          <xs:enumeration value="sexuality"/>
          <xs:enumeration value="soccer"/>
          <xs:enumeration value="sociology"/>
          <xs:enumeration value="sport"/>
          <xs:enumeration value="state"/>
          <xs:enumeration value="swimming"/>
          <xs:enumeration value="telecommunication"/>
          <xs:enumeration value="tennis"/>
          <xs:enumeration value="theatre"/>
          <xs:enumeration value="theology"/>
          <xs:enumeration value="transport"/>
          <xs:enumeration value="zoology"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <xs:attributeGroup name="attlist.Pragmatics">
    <xs:attribute name="chronology">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="neologism"/>
          <xs:enumeration value="oldfashioned"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="connotation">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="euphemistic"/>
          <xs:enumeration value="jocular"/>
          <xs:enumeration value="offensive"/>
          <xs:enumeration value="pejorative"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="geography">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="belg"/>
          <xs:enumeration value="ind"/>
          <xs:enumeration value="dialect"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="register">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="formal"/>
          <xs:enumeration value="informal"/>
          <xs:enumeration value="slang"/>
          <xs:enumeration value="vulgar"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
  </xs:attributeGroup>
  <!---->
  <xs:element name="Semantics-noun">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" maxOccurs="unbounded" ref="semanticShifts-noun"/>
      </xs:sequence>
      <xs:attributeGroup ref="attlist.Semantics-noun"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.Semantics-noun">
    <xs:attribute name="countability">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="count"/>
          <xs:enumeration value="coll"/>
          <xs:enumeration value="mass"/>
          <xs:enumeration value="plurtant"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="reference">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="common"/>
          <xs:enumeration value="proper"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="semanticType">
      <xs:simpleType>
        <xs:restriction base="xs:token">
          <xs:enumeration value="substance"/>
          <xs:enumeration value="abstract"/>
          <xs:enumeration value="animate"/>
          <xs:enumeration value="artefact"/>
          <xs:enumeration value="concrete"/>
          <xs:enumeration value="concrother"/>
          <xs:enumeration value="dynamic"/>
          <xs:enumeration value="human"/>
          <xs:enumeration value="institute"/>
          <xs:enumeration value="measure"/>
          <xs:enumeration value="nondynamic"/>
          <xs:enumeration value="nonhuman"/>
          <xs:enumeration value="place"/>
          <xs:enumeration value="time"/>
        </xs:restriction>
      </xs:simpleType>
    </xs:attribute>
    <xs:attribute name="semanticSubType"/>
  </xs:attributeGroup>
  <xs:element name="semanticShifts-noun">
    <xs:complexType>
      <xs:attributeGroup ref="attlist.semanticShifts-noun"/>
    </xs:complexType>
  </xs:element>
  <xs:attributeGroup name="attlist.semanticShifts-noun">
    <xs:attribute name="semanticType">
